<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>validate_annual_revenue</fullName>
    <active>true</active>
    <errorConditionFormula>OR(
   AnnualRevenue &lt; 0,
   AnnualRevenue &gt; 10000
)</errorConditionFormula>
    <errorMessage>annual revenue should be greater than 0 and less than 10000</errorMessage>
</ValidationRule>
